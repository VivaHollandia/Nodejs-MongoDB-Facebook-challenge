<%- include("./Layout/Header") %>

<form method="POST" action="/feed/edit/<%= result.id %>" style="margin: 150px;">
    <div class="mb-3">
      <label class="form-label">Name</label>
      <input name="name" value="<%= result.name %>" type="text" class="form-control">
    </div>
    <div class="mb-3">
      <label class="form-label">Message</label>
      <input name="message" value="<%= result.message %>" type="text" class="form-control">
    </div>
    <button type="submit" class="btn btn-outline-secondary">Submit</button>
</form>

<%- include("./Layout/Footer") %>